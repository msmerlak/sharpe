@article{zinn-justinMultiinstantons2004,
  title = {Multi-Instantons and Exact Results {{I}}: Conjectures, {{WKB}} Expansions, and Instanton Interactions},
  shorttitle = {Multi-Instantons and Exact Results {{I}}},
  author = {{Zinn-Justin}, Jean and Jentschura, Ulrich D.},
  year = {2004},
  month = sep,
  journal = {Annals of Physics},
